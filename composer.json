{
    "name": "winavin/magic-collection",
    "description": "It lets you call relationships directly on a collection of models and automatically merges the results into one collection.",
    "license": "MIT",
    "authors": [
        {
            "name": "Navin Patil",
            "email": "winavin@gmail.com",
            "homepage": "https://github.com/winavin"
        }
    ],
    "homepage": "https://github.com/winavin/magic-collection",
    "keywords": ["Laravel", "MagicCollection", "Laravel Magic Collection", "Magic Collection"],
    "require": {
        "laravel/framework": "^10.0|^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "Winavin\\MagicCollection\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Winavin\\MagicCollection\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Winavin\\MagicCollection\\MagicCollectionServiceProvider"
            ],
            "aliases": {
                "MagicCollection": "Winavin\\MagicCollection\\Facades\\MagicCollection"
            }
        }
    }
}
